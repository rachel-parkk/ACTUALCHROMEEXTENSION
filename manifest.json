{
    "manifest_version": 2,
    "name": "Countdown Timer and Link Blocker",
    "version": "1.0",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs",
      "webRequest",
      "webRequestBlocking",
      "storage",
      "activeTab",
      "*://*/*"
    ]
  }
  